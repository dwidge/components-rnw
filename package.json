{
  "name": "@dwidge/components-rnw",
  "version": "0.0.30",
  "description": "A collection of reusable React Native Web components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/components-rnw.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc -b . tsconfig.cjs.json",
    "test": "node --test --test-name-pattern"
  },
  "peerDependencies": {
    "@backpackapp-io/react-native-toast": "^0.13.0",
    "@dwidge/hooks-react": "^0.0.24",
    "@dwidge/utils-js": "^0.0.22",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-picker/picker": "^2.11.1",
    "@rneui/themed": "^4.0.0-rc.8",
    "react": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-web": "~0.20.0",
    "react-native-safe-area-context": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20"
  }
}
